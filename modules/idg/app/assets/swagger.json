    "/targets": {
      "get": {
         "tags": [
             "browse","target"
          ],
          "summary": "Browse targets by page",
          "description": "Paging through the targets",
          "parameters": [
             {
                "name": "top",
                "description": "Specify the number of targets per page",
                "in": "query",
		"type": "integer",
                "default": 10
             },
	     {
		 "name": "skip",
		 "description": "Specify the number of targets to skip",
		 "in": "query",
		 "type": "integer",
		 "default": 0
             }
          ],
          "responses": {
              "200": {
		  "description": "Successful response"
              },
              "404": {
		  "description": "Bad request"
              }
          }
       }
    },
    "/targets/search": {
      "get": {
         "tags": [
             "search","target"
         ],
         "summary": "Search targets",
         "description": "Search targets",
         "parameters": [
             {
                "name": "q",
                "description": "Search term",
                "in": "query",
                "type": "string",
		"default": ""
             },
             {
                "name": "top",
                "description": "Specify the number of targets per page",
                "in": "query",
		"type": "integer",
                "default": 10
             },
	     {
		 "name": "skip",
		 "description": "Specify the number of targets to skip",
		 "in": "query",
